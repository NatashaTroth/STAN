  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "server": "nodemon --exec babel-node app.js",
    "dev": "concurrently \"npm run server\" \"cd ../client/ && npm run start \"",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\" --quiet",
    "lintFix": "eslint \"**/*.js\" --fix",
    "build": "mkdir dist && babel app.js -d dist",
    "testDetectLeaks": "node --expose-gc ./node_modules/.bin/jest --watchAll --runInBand --detectOpenHandles",
    "test": "node --expose-gc ./node_modules/.bin/jest --watchAll",
    "ciTest": "jest",
    "testUnit": "jest --watch ./tests/unit",
    "testIntegration": "jest --watch ./tests/integration",
    "jscpd": "npx jscpd  -i \"**/+(node_modules|tests)/**\" /Users/natashatroth/Documents/FHS/MMP3/git/mmp3-stan/server",
    "jsinspect": "npx jsinspect /Users/natashatroth/Documents/FHS/MMP3/git/mmp3-stan/server --ignore \"node_modules|tests\""
  },
