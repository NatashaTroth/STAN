  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "server": "nodemon --exec babel-node app.js",
    "dev": "concurrently \"npm run server\" \"cd ../client/ && npm run start \"",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\" --quiet",
    "lintFix": "eslint \"**/*.js\" --fix",
    "build": "mkdir dist && babel src -s -d dist",
    "test": "jest --watchAll",
    "ciTest": "jest",
    "testUnit": "jest --watch ./tests/unit",
    "testIntegration": "jest --watch ./tests/integration --runInBand --detectOpenHandles"
  },
